<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zkzn.les.basicInfo.dao.InternalBillDao">
	
	<select id="listInternalBill" resultType="com.zkzn.les.basicInfo.pojo.InternalBill">
		select * from t_internal_bill
		<where>
			<if test="orderCode !=null and ''!= orderCode">and order_code=#{orderCode}</if>
			<if test="orderType !=null and ''!= orderType">and order_type=#{orderType}</if>
			<if test="describe !=null and ''!= describe">and describe like '%${describe}%'</if>
			
		</where>
	</select>
	
	
</mapper>